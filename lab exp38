import pandas as pd

# Create data dictionary
data = {
    'Name': ['John', 'Emma', 'Liam', 'Olivia'],
    'Age': [20, 21, 19, 22],
    'Marks': [85, 90, 78, 88]
}

# Define custom row labels
row_labels = ['Student_A', 'Student_B', 'Student_C', 'Student_D']

# Create DataFrame with custom index
df = pd.DataFrame(data, index=row_labels)

# Display DataFrame
print("DataFrame with Custom Index:\n")
print(df)
